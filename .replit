run = "npm run dev"
entrypoint = "src/app/page.tsx"
hidden = ["**/.config", "**/node_modules"]
modules = ["nodejs-20", "postgresql-16"]

[packager]
language = "nodejs"

  [packager.features]
  nodeVersion = "18"
  unitTests = false
  packageManager = "npm"
  guessImports = true
  enabledForHosting = false

[[ports]]
localPort = 5000
externalPort = 80

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[agent]
expertMode = true
